T.BK:AddToggle("ViewActive", {
    Title       = "ğŸ‘ï¸ Ativar View",
    Description = "Coloca sua cÃ¢mera na visÃ£o do jogador selecionado",
    Default     = false,
    Callback    = function(v)
        local cam = getCam()
        if not v then
            dis("view_lock")
            cam.CameraType = origCamType or Enum.CameraType.Custom
            cam.CameraSubject = origCamSubject or getHum()
            origCamSubject = nil; origCamType = nil
            notify("View", "CÃ¢mera restaurada.")
            return
        end

        local nome = Options.AlvoGeral and Options.AlvoGeral.Value
        local alvo = nome and Players:FindFirstChild(nome)
        if not alvo then notify("Erro", "Jogador nÃ£o encontrado."); return end

        local alvoHum = alvo.Character and alvo.Character:FindFirstChildOfClass("Humanoid")
        if not alvoHum then notify("Erro", alvo.Name .. " sem personagem."); return end

        origCamSubject    = cam.CameraSubject
        origCamType       = cam.CameraType
        cam.CameraType    = Enum.CameraType.Custom
        cam.CameraSubject = alvoHum

        con("view_lock", alvo.CharacterAdded, function(char)
            task.wait(1)
            local newHum = char:FindFirstChildOfClass("Humanoid")
            if newHum and Options.ViewActive and Options.ViewActive.Value then
                cam.CameraSubject = newHum
            end
        end)

        notify("View ğŸ‘ï¸", "CÃ¢mera em " .. alvo.Name .. "!\nDesative o toggle para voltar.")
    end,
})

T.BK:AddButton({
    Title = "ğŸ”„ Atualizar Lista (View / Fling)",
    Callback = function()
        local lista = playerList()
        dropAlvo:SetValues(lista);     dropAlvo:SetValue(lista[1])
        dropTpPlayer:SetValues(lista); dropTpPlayer:SetValue(lista[1])
        notify("Atualizado", #lista .. " jogador(es).")
    end,
})

-- â•â• FLING PLAYERS (100% FE) â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--
-- COMO O FLING FUNCIONA EM FE:
-- VocÃª NÃƒO pode mover o personagem de outro jogador do client.
-- O que vocÃª PODE fazer Ã© mover o SEU prÃ³prio personagem, que Ã©
-- replicado pelo servidor. Se seu HRP colidir com o deles em alta
-- velocidade, o servidor detecta e aplica o fling neles.
--
-- TÃ©cnica: BodyVelocity no seu HRP apontando pro alvo + loop de
-- re-posicionamento pra garantir colisÃ£o repetida atÃ© o fling acontecer.
-- O noclip Ã© desativado durante o fling pra garantir a colisÃ£o fÃ­sica.

T.BK:AddParagraph({
    Title   = "ğŸ’€ Fling Players",
    Content = "âš½ Bola  â€” Ram com bola equipada (colisÃ£o FE real)\nâ›µ Barco â€” Ram usando o veÃ­culo atual\nğŸ›‹ï¸ SofÃ¡  â€” TP no void junto com o alvo",
})

-- Slider de forÃ§a do fling
T.BK:AddSlider("FlingForce", {
    Title = "ForÃ§a do Fling", Description = "Quanto maior, mais longe voa. PadrÃ£o: 500",
    Default = 500, Min = 100, Max = 2000, Rounding = 0,
})

-- â”€â”€ FLING COM BOLA â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- Equipa a bola, desativa noclip temporariamente,
-- e spama o seu char em direÃ§Ã£o ao alvo com BodyVelocity.
-- A colisÃ£o fÃ­sica entre os chars Ã© replicada pelo servidor.

T.BK:AddButton({
    Title = "âš½ Fling com Bola",
    Description = "Equipa a bola e usa seu personagem como projÃ©til contra o alvo",
    Callback = function()
        local nome = Options.AlvoGeral and Options.AlvoGeral.Value
        local alvo = nome and Players:FindFirstChild(nome)
        if not alvo or not alvo.Character then
            notify("Erro", "Selecione um alvo vÃ¡lido."); return
        end

        local myRoot = getRoot()
        if not myRoot then return end

        -- Tenta equipar bola pra aumentar hitbox
        local ball = findTool("Ball", "Bola", "Soccer", "Football",
                              "SoccerBall", "BasketBall", "Balloon")
        local h = getHum()
        if h and ball and ball:IsA("Tool") then
            h:EquipTool(ball)
            task.wait(0.1)
        end

        local force = Options.FlingForce and Options.FlingForce.Value or 500

        notify("Fling âš½", "Ramando em " .. nome .. "... aguarde!")

        task.spawn(function()
            for i = 1, 40 do
                myRoot = getRoot()
                local alvoRoot = alvo.Character
                    and alvo.Character:FindFirstChild("HumanoidRootPart")
                if not myRoot or not alvoRoot then break end

                -- Desativa colisÃ£o do prÃ³prio char temporariamente
                -- para conseguir se posicionar dentro do alvo antes do ram
                for _, p in ipairs(getChar():GetDescendants()) do
                    if p:IsA("BasePart") then p.CanCollide = false end
                end

                -- Posiciona levemente atrÃ¡s do alvo
                local dir = (alvoRoot.Position - myRoot.Position)
                myRoot.CFrame = CFrame.new(alvoRoot.Position - dir.Unit * 1.5)

                task.wait(0.01)

                -- Reativa colisÃ£o e aplica velocidade absurda
                for _, p in ipairs(getChar():GetDescendants()) do
                    if p:IsA("BasePart") then p.CanCollide = true end
                end

                local bv = Instance.new("BodyVelocity", myRoot)
                bv.MaxForce = Vector3.new(1e9, 1e9, 1e9)
                -- DireÃ§Ã£o: em direÃ§Ã£o ao alvo + componente Y pra jogar pra cima
                local pushDir = (alvoRoot.Position - myRoot.Position + Vector3.new(0, 0.5, 0))
                bv.Velocity = pushDir.Unit * force
                Debris:AddItem(bv, 0.05)

                task.wait(0.05)
            end

            -- Restaura WalkSpeed apÃ³s o fling
            local hum = getHum()
            if hum and Options.WalkSpeed then
                hum.WalkSpeed = Options.WalkSpeed.Value
            end

            notify("Fling âš½", nome .. " deve ter saÃ­do voando!")
        end)
    end,
})

-- â”€â”€ FLING COM BARCO â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- VocÃª precisa estar DENTRO de um veÃ­culo.
-- O script seta velocidade mÃ¡xima e torque absurdos,
-- depois direciona o VehicleSeat em direÃ§Ã£o ao alvo via Throttle/Steer.
-- Como o veÃ­culo Ã© seu e estÃ¡ se movendo, a colisÃ£o Ã© replicada.

T.BK:AddButton({
    Title = "â›µ Fling com Barco/VeÃ­culo",
    Description = "Entre em um veÃ­culo primeiro â€” ele vai rmar no alvo automaticamente",
    Callback = function()
        local nome = Options.AlvoGeral and Options.AlvoGeral.Value
        local alvo = nome and Players:FindFirstChild(nome)
        if not alvo or not alvo.Character then
            notify("Erro", "Selecione um alvo vÃ¡lido."); return
        end

        local seat = findAnyVehicleSeat()
        if not seat then
            notify("Erro â›µ", "Entre em um veÃ­culo primeiro!\n(carro, barco, moto, etc)"); return
        end

        local force = Options.FlingForce and Options.FlingForce.Value or 500

        notify("Fling â›µ", "VeÃ­culo ramando em " .. nome .. "...")

        task.spawn(function()
            -- Seta velocidade absurda no seat
            pcall(function()
                seat.MaxSpeed = force * 4
                seat.Torque   = 9999
            end)

            for i = 1, 30 do
                local alvoRoot = alvo.Character
                    and alvo.Character:FindFirstChild("HumanoidRootPart")
                local myRoot = getRoot()
                if not alvoRoot or not myRoot then break end

                -- Aponta o veÃ­culo pro alvo via BodyVelocity no seat
                local dir = (alvoRoot.Position - myRoot.Position).Unit
                pcall(function()
                    local bv = Instance.new("BodyVelocity", seat)
                    bv.MaxForce = Vector3.new(1e9, 1e9, 1e9)
                    bv.Velocity = dir * force * 3
                    Debris:AddItem(bv, 0.07)
                end)

                task.wait(0.07)
            end

            -- Reseta velocidade do veÃ­culo
            pcall(function() seat.MaxSpeed = 80; seat.Torque = 200 end)
            notify("Fling â›µ", "Ram concluÃ­do! " .. nome .. " deve ter voado!")
        end)
    end,
})

-- â”€â”€ SOFÃ VOID â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- VocÃª equipa o sofÃ¡, vai pro alvo, ativa noclip,
-- e desce os dois pro void em loop via CFrame.
-- Como Ã© seu char se movendo + CFrame no alvo (local apenas),
-- o void kill real acontece quando o SERVER mata quem sai do mapa.
-- Seu char vai pro void e respawna; o alvo pode ou nÃ£o ir junto
-- dependendo de como o servidor do Brookhaven trata colisÃ£o de assentos.

T.BK:AddButton({
    Title = "ğŸ›‹ï¸ Void com SofÃ¡",
    Description = "Equipa o sofÃ¡ e vai pro void arrastando o alvo",
    Callback = function()
        local nome = Options.AlvoGeral and Options.AlvoGeral.Value
        local alvo = nome and Players:FindFirstChild(nome)
        if not alvo or not alvo.Character then
            notify("Erro", "Selecione um alvo vÃ¡lido."); return
        end

        local alvoRoot = alvo.Character:FindFirstChild("HumanoidRootPart")
        local myRoot   = getRoot()
        if not alvoRoot or not myRoot then return end

        local sofa = findTool("Sofa", "SofÃ¡", "Couch", "Furniture",
                              "Loveseat", "Armchair", "Chair", "Seat")
        if not sofa then
            notify("Erro ğŸ›‹ï¸", "Nenhum sofÃ¡ encontrado!\nPegue um sofÃ¡ no Brookhaven antes.")
            return
        end

        notify("Void ğŸ›‹ï¸", "Indo pro void com " .. nome .. "...\nDÃª respawn quando chegar lÃ¡.")

        task.spawn(function()
            -- Equipa sofÃ¡
            local h = getHum()
            if h and sofa:IsA("Tool") then
                h:EquipTool(sofa); task.wait(0.2)
            end

            -- Vai atÃ© o alvo
            myRoot.CFrame = alvoRoot.CFrame + Vector3.new(2, 0, 0)
            task.wait(0.1)

            -- Ativa noclip pra descer sem travar em objetos
            for _, p in ipairs(getChar():GetDescendants()) do
                if p:IsA("BasePart") then p.CanCollide = false end
            end

            local startPos = myRoot.Position
            local voidY    = -5000
            local steps    = 60

            for i = 1, steps do
                myRoot   = getRoot()
                alvoRoot = alvo.Character and alvo.Character:FindFirstChild("HumanoidRootPart")
                if not myRoot then break end

                local t    = i / steps
                -- Ease in: comeÃ§a devagar e vai acelerando
                local ease = t * t
                local newY = startPos.Y + (voidY - startPos.Y) * ease

                -- Move seu char pro void
                pcall(function()
                    myRoot.CFrame = CFrame.new(startPos.X, newY, startPos.Z)
                end)

                -- Tenta arrastar o alvo junto (local â€” pode nÃ£o funcionar em todos servidores)
                if alvoRoot then
                    pcall(function()
                        alvoRoot.CFrame = CFrame.new(startPos.X + 2, newY, startPos.Z)
                    end)
                end

                task.wait(0.02)
            end

            -- Restaura colisÃ£o
            local c = getChar()
            if c then
                for _, p in ipairs(c:GetDescendants()) do
                    if p:IsA("BasePart") then p.CanCollide = true end
                end
            end

            notify("Void ğŸ›‹ï¸", "Chegou no void! DÃª respawn para voltar.")
        end)
    end,
})

-- â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
-- â•‘                     ABA  VISUAL                            â•‘
-- â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

