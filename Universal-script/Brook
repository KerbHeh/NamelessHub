T.BK:AddSlider("CarSpeed", {
    Title = "Velocidade MÃ¡xima", Description = "PadrÃ£o Brookhaven: ~80",
    Default = 80, Min = 0, Max = 3000, Rounding = 0,
    Callback = function(v)
        local s = getVehicleSeat()
        if s then s.MaxSpeed = v; notify("Carro", "MaxSpeed â†’ " .. v)
        else notify("Erro", "Entre em um carro primeiro!") end
    end,
})

T.BK:AddSlider("CarTorque", {
    Title = "Torque (aceleraÃ§Ã£o)", Description = "PadrÃ£o: ~200",
    Default = 200, Min = 0, Max = 9999, Rounding = 0,
    Callback = function(v)
        local s = getVehicleSeat()
        if s then s.Torque = v end
    end,
})

T.BK:AddToggle("CarLoop", {
    Title = "Manter velocidade automaticamente",
    Description = "Re-aplica a cada frame enquanto vocÃª dirige",
    Default = false,
    Callback = function(v)
        dis("carloop")
        if not v then return end
        con("carloop", RunService.Heartbeat, function()
            local s = getVehicleSeat(); if not s then return end
            if Options.CarSpeed  then s.MaxSpeed = Options.CarSpeed.Value  end
            if Options.CarTorque then s.Torque   = Options.CarTorque.Value end
        end)
    end,
})

T.BK:AddButton({
    Title = "âœ… Aplicar no Carro Atual",
    Description = "Aplica Speed e Torque de uma vez",
    Callback = function()
        local s = getVehicleSeat()
        if not s then notify("Erro", "Entre em um carro primeiro!"); return end
        s.MaxSpeed = Options.CarSpeed  and Options.CarSpeed.Value  or 500
        s.Torque   = Options.CarTorque and Options.CarTorque.Value or 1000
        notify("Carro", ("Aplicado! Speed: %d | Torque: %d"):format(s.MaxSpeed, s.Torque))
    end,
})

T.BK:AddButton({
    Title = "ğŸ” Resetar Carro (padrÃ£o)",
    Callback = function()
        local s = getVehicleSeat()
        if s then s.MaxSpeed = 80; s.Torque = 200; notify("Carro", "Resetado ao padrÃ£o.")
        else notify("Erro", "Entre em um carro primeiro!") end
    end,
})

-- â•â• 2. SPY PLAYER â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

T.BK:AddParagraph({
    Title   = "ğŸ” Spy Player",
    Content = "Monitora HP, posiÃ§Ã£o, ferramenta e distÃ¢ncia de um jogador.",
})

local spyGui = nil

local function buildSpyUI(plr)
    if spyGui then spyGui:Destroy() end
    spyGui = Instance.new("ScreenGui", LP.PlayerGui)
    spyGui.Name           = "_SpyHD"
    spyGui.ResetOnSpawn   = false
    spyGui.IgnoreGuiInset = true
    spyGui.DisplayOrder   = 999

    -- Fundo arredondado
    local panel = Instance.new("Frame", spyGui)
    panel.Size                  = UDim2.fromOffset(280, 160)
    panel.Position              = UDim2.new(1, -292, 0, 12)
    panel.BackgroundColor3      = Color3.fromRGB(10, 10, 15)
    panel.BackgroundTransparency= 0.08
    panel.BorderSizePixel       = 0
    Instance.new("UICorner", panel).CornerRadius = UDim.new(0, 10)

    -- Barra de tÃ­tulo
    local bar = Instance.new("Frame", panel)
    bar.Size             = UDim2.new(1, 0, 0, 32)
    bar.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    bar.BorderSizePixel  = 0
    Instance.new("UICorner", bar).CornerRadius = UDim.new(0, 10)

    local title = Instance.new("TextLabel", bar)
    title.Size               = UDim2.fromScale(1, 1)
    title.BackgroundTransparency = 1
    title.Text               = "ğŸ”  Spy: " .. plr.DisplayName .. "  (@" .. plr.Name .. ")"
    title.TextColor3         = Color3.fromRGB(255, 215, 60)
    title.TextScaled         = true
    title.Font               = Enum.Font.GothamBold
    title.TextXAlignment     = Enum.TextXAlignment.Center

    -- Linhas de info
    local function row(yPos, cor)
        local lbl = Instance.new("TextLabel", panel)
        lbl.Size               = UDim2.new(1, -16, 0, 24)
        lbl.Position           = UDim2.fromOffset(8, yPos)
        lbl.BackgroundTransparency = 1
        lbl.TextColor3         = cor
        lbl.TextScaled         = true
        lbl.Font               = Enum.Font.Gotham
        lbl.TextXAlignment     = Enum.TextXAlignment.Left
        lbl.Text               = "Carregando..."
        return lbl
    end

    local rHP   = row(36,  Color3.fromRGB(100, 255, 120))
    local rPos  = row(64,  Color3.fromRGB(100, 180, 255))
    local rTool = row(92,  Color3.fromRGB(255, 190, 80))
    local rDist = row(120, Color3.fromRGB(200, 200, 200))
    local rState= row(144, Color3.fromRGB(160, 160, 255))

    con("spy", RunService.Heartbeat, function()
        local char = plr.Character
        if not char then rHP.Text = "â¤  Offline"; return end

        local hum  = char:FindFirstChildOfClass("Humanoid")
        local root = char:FindFirstChild("HumanoidRootPart")
        local mine = getRoot()

        -- HP
        if hum then
            local pct = hum.Health / hum.MaxHealth
            rHP.TextColor3 = Color3.fromRGB(math.floor(255*(1-pct)), math.floor(255*pct), 50)
            rHP.Text = ("â¤  %d / %d   (%d%%)"):format(
                math.floor(hum.Health), math.floor(hum.MaxHealth), math.floor(pct*100))
        end

        -- PosiÃ§Ã£o
        if root then
            local p = root.Position
            rPos.Text = ("ğŸ“  X:%.1f  Y:%.1f  Z:%.1f"):format(p.X, p.Y, p.Z)
            if mine then
                rDist.Text = ("ğŸ“  DistÃ¢ncia: %.1f studs"):format(
                    (root.Position - mine.Position).Magnitude)
            end
        end

        -- Ferramenta
        local tool = char:FindFirstChildOfClass("Tool")
        rTool.Text = tool and ("ğŸ”§  " .. tool.Name) or "ğŸ”§  Sem ferramenta"

        -- Estado
        if hum then
            local states = {
                [Enum.HumanoidStateType.Running]  = "ğŸƒ Correndo",
                [Enum.HumanoidStateType.Jumping]  = "ğŸ¦˜ Pulando",
                [Enum.HumanoidStateType.Freefall] = "ğŸª‚ Caindo",
                [Enum.HumanoidStateType.Seated]   = "ğŸª‘ Sentado",
                [Enum.HumanoidStateType.Swimming] = "ğŸŠ Nadando",
                [Enum.HumanoidStateType.Dead]     = "ğŸ’€ Morto",
            }
            rState.Text = states[hum:GetState()] or "â“ Desconhecido"
        end
    end)
end

