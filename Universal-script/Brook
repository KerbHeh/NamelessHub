-- BotÃµes de clima
local function setWeather(ct, bright, fe, amb)
    Lighting.ClockTime  = ct
    Lighting.Brightness = bright
    Lighting.FogEnd     = fe
    Lighting.Ambient    = Color3.fromRGB(table.unpack(amb))
end

