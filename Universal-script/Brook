local function safeLoad(url, nome)
    local ok, res = pcall(function()
        return loadstring(game:HttpGet(url, true))()
    end)
    if not ok then error("[AdminHD] Falhou: " .. nome .. "\n" .. tostring(res), 0) end
    return res
end

