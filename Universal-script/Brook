    Title = "Anti-AFK", Description = "Nunca toma kick por inatividade",
    Default = false,
    Callback = function(v)
        dis("afk")
        if v then
            con("afk", LP.Idled, function()
                VirtualUser:Button2Down(Vector2.new(0, 0), getCam().CFrame)
                task.wait(0.1)
                VirtualUser:Button2Up(Vector2.new(0, 0), getCam().CFrame)
            end)
        end
    end,
})

