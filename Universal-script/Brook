    Title = "ğŸ” Resetar Mundo",
    Callback = function()
        workspace.Gravity = 196.2
        Options.Gravity:SetValue(196)
        for k, val in pairs(ORIG) do Lighting[k] = val end
        notify("Reset", "Mundo restaurado ao original.")
    end,
})

