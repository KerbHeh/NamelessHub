-- â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
-- â•‘        RUNTIME LOOP (Heartbeat)      â•‘
-- â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local tickers = {
    steal    = 0,
    sell     = 0,
    upgrade  = 0,
    esp      = 0,
    wheel    = 0,
    autoTP   = 0,
}

local brainrotKeywords = {"Brainrot","Brain","Drop","Loot","Rare","Mythic","Common","Epic","Legendary","Pickup"}
local dropKeywords     = {"Drop","Loot","Rare","Mythic","Legendary","Chest","Crate","Pickup"}

RunService.Heartbeat:Connect(function(dt)
    local ok = root and hum and char

    -- â”€â”€ Speed lock â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    if ok and Options.WalkSpeed and hum.WalkSpeed ~= Options.WalkSpeed.Value then
        SafeCall(function() hum.WalkSpeed = Options.WalkSpeed.Value end)
    end

    -- â”€â”€ God Mode â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    if ok and Options.GodMode and Options.GodMode.Value then
        SafeCall(function()
            hum.MaxHealth = math.huge
            if hum.Health < 1e10 then hum.Health = math.huge end
        end)
    end

    -- â”€â”€ Noclip â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    if ok and Options.Noclip and Options.Noclip.Value then
        SafeCall(function()
            for _, p in ipairs(char:GetDescendants()) do
                if p:IsA("BasePart") then p.CanCollide = false end
            end
        end)
    end

    -- â”€â”€ Anti Void â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    if ok and Options.AntiVoid and Options.AntiVoid.Value then
        if root.Position.Y < -80 then
            root.CFrame = CFrame.new(root.Position.X, 50, root.Position.Z)
        end
    end

    -- â”€â”€ Fly movement â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    if Options.Fly and Options.Fly.Value and FlyBV and FlyBG and root then
        local vel = Vector3.zero
        local spd = Options.FlySpeed and Options.FlySpeed.Value or 100
        local cf  = Camera.CFrame
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then vel = vel + cf.LookVector  end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then vel = vel - cf.LookVector  end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then vel = vel - cf.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then vel = vel + cf.RightVector end
        if UserInputService:IsKeyDown(Enum.KeyCode.E) then vel = vel + Vector3.new(0,1,0) end
        if UserInputService:IsKeyDown(Enum.KeyCode.Q) then vel = vel - Vector3.new(0,1,0) end
        FlyBV.Velocity = vel * spd
        FlyBG.CFrame   = cf
    end

    -- â”€â”€ Fullbright â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    if Options.Fullbright and Options.Fullbright.Value then
        SafeCall(function()
            Lighting.Brightness    = 10
            Lighting.ClockTime     = 14
            Lighting.FogEnd        = 100000
            Lighting.FogStart      = 99999
            Lighting.GlobalShadows = false
            Lighting.Ambient       = Color3.fromRGB(178,178,178)
        end)
    end

    -- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    -- AUTO STEAL LOOP (0.15s)
    tickers.steal += dt
    if tickers.steal >= 0.15 then
        tickers.steal = 0
        local range = Options.StealRange and Options.StealRange.Value or 50

        if ok and Options.AutoSteal and Options.AutoSteal.Value then
            SafeCall(function()
                for _, obj in ipairs(Workspace:GetDescendants()) do
                    for _, kw in ipairs(brainrotKeywords) do
                        if obj.Name:find(kw) and DistTo(obj) <= range then
                            local pos = obj:IsA("BasePart") and obj.Position
                                     or (obj:IsA("Model") and obj.PrimaryPart and obj.PrimaryPart.Position)
                            if pos then
                                root.CFrame = CFrame.new(pos + Vector3.new(0,2,0))
                                FireRemote("Grab",    obj)
                                FireRemote("Steal",   obj)
                                FireRemote("Collect", obj)
                                FireRemote("PickUp",  obj)
                            end
                            break
                        end
                    end
                end
            end)
        end

        -- Auto Steal from Players
        if ok and Options.AutoStealFromPlayers and Options.AutoStealFromPlayers.Value then
            SafeCall(function()
                local priority = Options.StealPriority and Options.StealPriority.Value or "Mais PrÃ³ximo"
                local target
                if priority == "Mais Rico (Leaderstats)" then
                    target = GetRichestPlayer()
                elseif priority == "Mais PrÃ³ximo" then
                    target = GetNearestPlayer()
                elseif priority == "AleatÃ³rio" then
                    local all = {}
                    for _, p in ipairs(Players:GetPlayers()) do
                        if p ~= lp then table.insert(all, p) end
                    end
                    target = all[math.random(1, math.max(1, #all))]
                else
                    -- Menor HP
                    local lowestHP, best2 = math.huge, nil
                    for _, p in ipairs(Players:GetPlayers()) do
                        if p ~= lp and p.Character then
                            local ph = p.Character:FindFirstChildOfClass("Humanoid")
                            if ph and ph.Health < lowestHP then
                                lowestHP = ph.Health best2 = p
                            end
                        end
                    end
                    target = best2
                end

                if target and target.Character then
                    local tRoot = target.Character:FindFirstChild("HumanoidRootPart")
                    if tRoot and (root.Position - tRoot.Position).Magnitude <= range then
                        FireRemote("StealFrom",  target)
                        FireRemote("Steal",      target)
                        FireRemote("GrabPlayer", target)
                    end
                end
            end)
        end

        -- Auto Collect Drops
        if ok and Options.AutoCollectDrops and Options.AutoCollectDrops.Value then
            SafeCall(function()
                for _, obj in ipairs(Workspace:GetDescendants()) do
                    for _, kw in ipairs(dropKeywords) do
                        if obj.Name:find(kw) and DistTo(obj) <= range * 2 then
                            local pos = obj:IsA("BasePart") and obj.Position
                                     or (obj:IsA("Model") and obj.PrimaryPart and obj.PrimaryPart.Position)
                            if pos then
                                root.CFrame = CFrame.new(pos + Vector3.new(0,2,0))
                                FireRemote("PickUp",  obj)
                                FireRemote("Collect", obj)
                            end
                            break
                        end
                    end
                end
            end)
        end
    end

    -- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    -- AUTO SELL (2s)
    tickers.sell += dt
    if tickers.sell >= 2 then
        tickers.sell = 0
        if ok and Options.AutoSell and Options.AutoSell.Value then
            SafeCall(function()
                local remotes = {"Sell","SellAll","SellBrainrot","SellItems","SellInventory"}
                for _, name in ipairs(remotes) do FireRemote(name) end
                local gui = lp:FindFirstChild("PlayerGui")
                if gui then
                    for _, btn in ipairs(gui:GetDescendants()) do
                        if (btn:IsA("TextButton") or btn:IsA("ImageButton")) and btn.Name:lower():find("sell") then
                            SafeCall(function() btn.MouseButton1Click:Fire() end)
                        end
                    end
                end
            end)
        end
    end

    -- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    -- AUTO REBIRTH (1.5s)
    -- (handled separately below via dedicated spawn)

    -- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    -- UPGRADES LOOP (0.5â€“5s)
    tickers.upgrade += dt
    local upgDelay = Options.UpgradeDelay and Options.UpgradeDelay.Value or 0.5
    if tickers.upgrade >= upgDelay then
        tickers.upgrade = 0
        for _, upg in ipairs(upgradeList) do
            local optKey = "Auto_" .. upg.key
            if Options[optKey] and Options[optKey].Value then
                SafeCall(function()
                    FireRemote("Upgrade"  .. upg.key)
                    FireRemote("Buy"      .. upg.key)
                    FireRemote("Purchase" .. upg.key)
                    FireRemote("Upgrade", upg.key)
                end)
            end
        end
    end

    -- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    -- AUTO SPIN WHEEL (3s)
    tickers.wheel += dt
    if tickers.wheel >= 3 then
        tickers.wheel = 0
        if ok and Options.AutoWheel and Options.AutoWheel.Value then
            SafeCall(function()
                FireRemote("SpinWheel")
                FireRemote("Wheel")
                FireRemote("LuckyWheel")
                local gui = lp:FindFirstChild("PlayerGui")
                if gui then
                    for _, btn in ipairs(gui:GetDescendants()) do
                        if (btn:IsA("TextButton") or btn:IsA("ImageButton")) and btn.Name:lower():find("spin") then
                            SafeCall(function() btn.MouseButton1Click:Fire() end)
                        end
                    end
                end
            end)
        end
    end

    -- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    -- AUTO TP RICHEST (2s)
    tickers.autoTP += dt
    if tickers.autoTP >= 2 then
        tickers.autoTP = 0
        if ok and Options.AutoTPRichest and Options.AutoTPRichest.Value then
            SafeCall(function()
                local richest = GetRichestPlayer()
                if richest and richest.Character then
                    local tRoot = richest.Character:FindFirstChild("HumanoidRootPart")
                    if tRoot then
                        root.CFrame = tRoot.CFrame + Vector3.new(0, 3, 4)
                    end
                end
            end)
        end
    end

    -- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
    -- ESP UPDATE (0.5s)
    tickers.esp += dt
    if tickers.esp >= 0.5 then
        tickers.esp = 0

        -- ESP Brainrots
        if Options.ShowBrainrots and Options.ShowBrainrots.Value then
            local col = Options.ESPBrainrotColor and Options.ESPBrainrotColor.Value or Color3.fromRGB(180,60,255)
            for _, obj in ipairs(Workspace:GetDescendants()) do
                for _, kw in ipairs(brainrotKeywords) do
                    if obj.Name:find(kw) and not obj:IsA("Humanoid") then
                        AddESP(obj, col, Color3.new(1,1,1), "brainrot")
                        break
                    end
                end
            end
        else
            ClearESPByTag("brainrot")
        end

        -- ESP Players
        if Options.ShowPlayers and Options.ShowPlayers.Value then
            local col    = Options.ESPPlayerColor and Options.ESPPlayerColor.Value or Color3.fromRGB(255,60,60)
            local showStats = Options.ShowLeaderStats and Options.ShowLeaderStats.Value
            for _, p in ipairs(Players:GetPlayers()) do
                if p ~= lp and p.Character then
                    AddESP(p.Character, col, Color3.new(1,1,1), "player")
                    -- Atualizar label
                    local espData = ESPObjects[p.Character]
                    if espData and espData.label then
                        local br = GetStat(p, "Brainrots")
                        local cash = GetStat(p, "Cash") + GetStat(p, "Coins")
                        if showStats then
                            espData.label.Text = p.Name .. "\nğŸ§  " .. br .. "  ğŸ’° " .. cash
                        else
                            espData.label.Text = p.Name
                        end
                    end
                end
            end
        else
            ClearESPByTag("player")
        end

        -- ESP Drops
        if Options.ShowDrops and Options.ShowDrops.Value then
            for _, obj in ipairs(Workspace:GetDescendants()) do
                for _, kw in ipairs(dropKeywords) do
                    if obj.Name:find(kw) then
                        AddESP(obj, Color3.fromRGB(255,215,0), Color3.fromRGB(255,255,0), "drop")
                        break
                    end
                end
            end
        else
            ClearESPByTag("drop")
        end
    end
end)

-- â”€â”€ Infinite Jump â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
UserInputService.JumpRequest:Connect(function()
    if Options.InfiniteJump and Options.InfiniteJump.Value and hum then
        hum:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

-- â”€â”€ Auto Rebirth (separado â€” 2s) â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
task.spawn(function()
    while task.wait(2) do
        if Options.AutoRebirth and Options.AutoRebirth.Value then
            SafeCall(function()
                FireRemote("Rebirth")
                FireRemote("DoRebirth")
                FireRemote("PrestigeRebirth")
                local gui = lp:FindFirstChild("PlayerGui")
                if gui then
                    for _, btn in ipairs(gui:GetDescendants()) do
                        if (btn:IsA("TextButton") or btn:IsA("ImageButton")) and btn.Name:lower():find("rebirth") then
                            SafeCall(function() btn.MouseButton1Click:Fire() end)
                        end
                    end
                end
            end)
        end
    end
end)

-- â”€â”€ Limpar ESP de jogadores que saÃ­ram â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
Players.PlayerRemoving:Connect(function(p)
    if p.Character then RemoveESP(p.Character) end
end)

